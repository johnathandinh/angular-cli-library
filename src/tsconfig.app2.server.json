{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "paths": {
              "@angular-cli-library/*": ["../node_modules/angular-cli-library/*"]
        },
        "outDir": "../out-tsc/app",
        "baseUrl": ".",
        "module": "commonjs",
        "types": []
    },
    "include": [
        "app2/**/*",
        "../node_modules/angular-cli-library/**/*"
    ],
    "exclude":[
        "../**/*.spec.ts"
    ],
    "angularCompilerOptions": {
        "entryModule": "app2/app/app.server.module#AppServerModule"
    }
}
