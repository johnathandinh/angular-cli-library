{
  "name": "angular-cli-library",
  "version": "1.5.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "library:publish": "npm publish src/shared",
    "update": "node scripts/update.js",
    "verify": "node scripts/verify.js",
    "app1-browser": "ng serve --app app1-browser --port 4200",
    "app2-browser": "ng serve --app app2-browser --port 4201",
    "app1-browser:build": "ng build --app app1-browser --prod --build-optimizer",
    "app2-browser:build": "ng build --app app2-browser --prod --build-optimizer",
    "app1-server": "npm run app1-browser:build && npm run app1-server:build && webpack --config webpack.app1.server.config.js --progress --colors && node dist/app1-server/server.js",
    "app2-server": "ng build --app app2-server --prod --build-optimizer",
    "app1-server:build": "ng build --app app1-server --prod --build-optimizer --output-hashing=false",
    "app2-server:build": "ng build --app app2-server --prod --build-optimizer --output-hashing=false",
    "app1:test": "ng test --app app1",
    "app2:test": "ng test --app app2",
    "app1:e2e": "ng e2e --app app1",
    "app2:e2e": "ng e2e --app app2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.2",
    "@angular/router": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "angular-cli-library": "1.3.2",
    "core-js": "2.4.1",
    "rxjs": "^5.5.2",
    "ts-loader": "^3.1.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
